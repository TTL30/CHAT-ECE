{
  "name": "ece-back-end",
  "version": "0.0.0",
  "description": "WebTech back-end project",
  "author": "tiago teixeira",
  "dependencies": {
    "accountdown": "^4.1.0",
    "accountdown-basic": "^1.1.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "level": "^6.0.1",
    "microtime": "^3.0.0",
    "mixme": "^0.3.5",
    "nodemon": "^2.0.6",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "mocha": "^8.1.3",
    "concurrently": "^5.3.0"
  },
  "keywords": [
    "tech",
    "ece",
    "nodejs"
  ],
  "license": "MIT",
  "main": "./lib/index.js",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should"
    ],
    "inline-diffs": true,
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "scripts": {
    "reset": "bin/reset",
    "develop": "bin/init && bin/start",
    "start": "nodemon lib/app.js",
    "client": "npm run start --prefix ../front-end",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  }
}
